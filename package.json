{
	"name": "pxtovwvh",
	"displayName": "pxtovwvh",
	"description": "px to vw and vh of css3 units",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:html",
		"onLanguage:vue",
		"onLanguage:css",
		"onLanguage:less",
		"onLanguage:scss",
		"onLanguage:sass",
		"onLanguage:stylus"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
				"type": "object",
				"title": "px2vwvh configure",
				"properties": {
						"px2vwvh.width": {
								"type": "number",
								"default": 1920,
								"description": "design width"
						},
						"px2vwvh.height": {
							"type": "number",
							"default": 920,
							"description": "design height"
						},
						"px2vwvh.toFixedNum": {
								"type": "number",
								"default": 4,
								"description": "px2vw 保留小数长度"
						}
				}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
